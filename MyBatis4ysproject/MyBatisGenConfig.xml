<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry location="mysql-connector-java-5.1.6.jar" />

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
            connectionURL="jdbc:mysql://apexsoft-svr1.iptime.org:3306/ysproject"
            userId="ysproject"
            password="ysproject">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.apexsoft.ysprj.user.domain" targetProject="ysproject/src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.apexsoft.ysprj.user.sqlmap"  targetProject="ysproject/src/main/java">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        <!--
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.apexsoft.ysprj.user.dao"  targetProject="ysproject/src/main/java">
          <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        -->
        <table schema="ysproject" tableName="COMM_CODE" domainObjectName="CommonCode" >
        </table>

    </context>
</generatorConfiguration>
