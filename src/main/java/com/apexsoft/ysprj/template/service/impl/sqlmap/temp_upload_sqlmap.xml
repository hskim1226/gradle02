<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TEMP_FILE">

    <resultMap id="tempFile" type="com.apexsoft.ysprj.template.service.TempFileVO" >
        <result property="seq" column="seq"/>
        <result property="path" column="path"/>
        <result property="fileName" column="file_name"/>
    </resultMap>

    <insert id="insert" parameterType="com.apexsoft.ysprj.template.service.TempFileVO">
        <![CDATA[
            insert into temp_file (path, file_name)
            values (#{path}, #{fileName})
		]]>
    </insert>

    <select id="selectList" resultMap="tempFile" >
        <![CDATA[
		  SELECT seq, path, file_name
		    FROM temp_file
			WHERE 1=1
		]]>
    </select>

    <select id="selectByPk" parameterType="int" resultMap="tempFile" >
        <![CDATA[
		  SELECT seq, path, file_name
		    FROM temp_file
			WHERE seq = #{seq}
		]]>
    </select>

</mapper>