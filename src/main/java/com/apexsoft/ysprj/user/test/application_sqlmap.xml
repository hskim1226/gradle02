<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apexsoft.ysprj.test.application.Mapper">

    <select id="selectByApplNo" resultType="com.apexsoft.ysprj.user.test.Application">
        <![CDATA[
		  SELECT appl_no,
		         user_id,
		         user_name,
		         phone_number,
		         address
		    FROM tmp_application
		  WHERE appl_no = #{applNo}
		  ORDER BY appl_no
		]]>
    </select>

</mapper>