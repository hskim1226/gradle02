<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.apexsoft.ysprj.applicants.common.sqlmap.CustomMailMapper" >

  <resultMap id="mailInfoMap" type="com.apexsoft.ysprj.applicants.common.domain.MailInfo" >
    <id column="USER_ID" property="userId" jdbcType="VARCHAR" />
    <result column="CAMP_NAME" property="campName" jdbcType="VARCHAR" />
    <result column="CAMP_NAME_XXEN" property="campNameXxen" jdbcType="VARCHAR" />
    <result column="COLL_NAME" property="collName" jdbcType="VARCHAR" />
    <result column="COLL_NAME_XXEN" property="collNameXxen" jdbcType="VARCHAR" />
    <result column="DEPT_NAME" property="deptName" jdbcType="VARCHAR" />
    <result column="DEPT_NAME_XXEN" property="deptNameXxen" jdbcType="VARCHAR" />
    <result column="APPL_NO" property="applNo" jdbcType="INTEGER" />
    <result column="APPL_ID" property="applId" jdbcType="VARCHAR" />
    <result column="ADMS_NO" property="admsNo" jdbcType="VARCHAR" />
    <result column="KOR_NAME" property="korName" jdbcType="VARCHAR" />
    <result column="ENG_SUR" property="engSur" jdbcType="VARCHAR" />
    <result column="ENG_NAME" property="engName" jdbcType="VARCHAR" />
  </resultMap>

  <select id="selectApplicationCompletedMailInfo" resultMap="mailInfoMap" parameterType="int">
    select A.CAMP_NAME, A.CAMP_NAME_XXEN, B.COLL_NAME, B.COLL_NAME_XXEN, C.DEPT_NAME, C.DEPT_NAME_XXEN, D.USER_ID, D.APPL_NO, D.APPL_ID, D.ADMS_NO, D.KOR_NAME, D.ENG_SUR, D.ENG_NAME
    from CAMP A, COLL B, DEPT C, APPL D
    where A.CAMP_CODE = B.CAMP_CODE
    and   B.COLL_CODE = C.COLL_CODE
    and   C.DEPT_CODE = D.DEPT_CODE
    and   D.APPL_NO = #{applNo,jdbcType=INTEGER}
    and   D.APPL_STS_CODE = '00020';
  </select>

</mapper>